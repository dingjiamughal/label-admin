{
    "root": true,
    "env": {
        "node": true
    },
    "extends": [
        "plugin:vue/essential",
        "eslint:recommended"
    ],
    "rules": {
        "indent": [2, 4],
        "no-console": "off",
        "no-unused-vars": "off"
    },
    "parserOptions": {
        "parser": "babel-eslint"
    }
}
